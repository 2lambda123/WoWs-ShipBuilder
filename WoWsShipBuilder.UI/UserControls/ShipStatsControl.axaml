<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:t="clr-namespace:WoWsShipBuilder.UI.Translations"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WoWsShipBuilder.UI.UserControls.ShipStatsControl"
             xmlns:vm="clr-namespace:WoWsShipBuilder.UI.ViewModels"
             xmlns:data="clr-namespace:WoWsShipBuilderDataStructures;assembly=WoWsShipBuilderDataStructures">
    <Design.DataContext>
        <vm:ShipStatsControlViewModel/>
    </Design.DataContext>
    <UserControl.Styles>
        <Style Selector="TextBlock.RightSide">
            <Setter Property="TextAlignment" Value="Right"/>
            <Setter Property="Grid.Column" Value="2"/>
        </Style>
        <Style Selector="StackPanel.Values > TextBlock">
            <Setter Property="TextAlignment" Value="Right"/>
        </Style>
    </UserControl.Styles>
    <ScrollViewer>
        <StackPanel Orientation="Horizontal">
            <!-- Main Armaments -->
            <StackPanel Orientation="Vertical" Spacing="10">
                <!--Main guns-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="{x:Static t:Translation.ShipStats_MainBattery}"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <!--replace grid with stackpanel direction vertical for each row add another stackpanel direction horizontal spacing 30 if stackpanel doesnt work use dockpanel-->
                    <StackPanel Orientation="Vertical">
                        <DockPanel>
                            <TextBlock Text="Main battery setup binding" DockPanel.Dock="Top"/>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Range}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Reload}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_RateOfFire}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_DegreesTurn}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_TraverseSpeed}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Sigma}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_HorizontalDisp}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_VerticalDisp}"/>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Right" HorizontalAlignment="Right" Classes="Values">
                                <TextBlock Text="{Binding Range}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding ReloadTime}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding RateOfFire}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding DegreesTurnTime}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding TraverseSpeed}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding Sigma}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding ShipStats_HorizontalDisp}" TextAlignment="Right"/>
                                <TextBlock Text="{Binding ShipStats_VerticalDisp}" TextAlignment="Right"/>
                            </StackPanel>
                        </DockPanel>
                        <Separator Margin="0,5"/>
                        <ItemsControl Items="{Binding MainBatteryModuleList}">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate DataType="data:ArtilleryShell">
                                    <DockPanel>
                                        <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                            <TextBlock Text="{Binding Name}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_Damage}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_TheoreticalDPM}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_ShellVelocity}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_ShellWeight}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_Penetration}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_FireChance}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_Ricochet}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_Overmatch}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_ArmingThreshold}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_FuseTimer}"/>
                                            <TextBlock Text="{x:Static t:Translation.ShipStats_DepthExplosion}"/>
                                        </StackPanel>
                                        <StackPanel Orientation="Vertical" DockPanel.Dock="Right" HorizontalAlignment="Right" Classes="Values">
                                            <TextBlock Text="{Binding ShellType}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding Damage}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding TheoreticalDPM}" TextAlignment="Right"/>
                                            
                                            <!--Not sure if this is the correct prop, should be initial shell speed-->
                                            <TextBlock Text="{Binding AirDrag}" TextAlignment="Right"/>
                                            
                                            <TextBlock Text="{Binding Mass}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding Penetration}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding FireChance}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding RicochetAngle}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding Overmatch}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding ArmingThreshold}" TextAlignment="Right"/>
                                            <TextBlock Text="{Binding FuseTimer}" TextAlignment="Right"/>
                                            <!--No clue which prop this should be-->
                                            <TextBlock Text="{Binding DepthExplosionRadius}" TextAlignment="Right"/>
                                        </StackPanel>
                                    </DockPanel>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </StackPanel>
                </Expander>
                <!--Torpedoes-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="{x:Static t:Translation.ShipStats_Torpedo}"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <StackPanel Orientation="Vertical">
                        <DockPanel>
                            <TextBlock Text="Torpedo setup" DockPanel.Dock="Top"></TextBlock>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Range}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Damage}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Reload}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Speed}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Detectability}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_ReactionTime}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FloodingChance}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_DegreesTurn}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_TraverseSpeed}"/>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Right">
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                            </StackPanel>
                        </DockPanel>
                    </StackPanel>
                </Expander>

                <!--Airstrikes-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="{x:Static t:Translation.ShipStats_Airstrike}"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <StackPanel Orientation="Vertical">
                        <DockPanel>
                            <TextBlock Text="Airstrike name" DockPanel.Dock="Top"></TextBlock>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                <TextBlock Text="{x:Static t:Translation.ShipStats_AvailableFlights}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Reload}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_AircraftHP}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_BompPayload}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_BombType}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Damage}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Penetration}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FireChance}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Range}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Delay}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Speed}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_DepthExplosion}"/>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Right">
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                            </StackPanel>
                        </DockPanel>
                    </StackPanel>
                </Expander>
            </StackPanel>

            <!-- Secondary Armaments -->
            <StackPanel Orientation="Vertical" Spacing="10">
                <!--Secondary-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Secondary Battery"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*">
                        <TextBlock Text="{x:Static t:Translation.ShipStats_Range}" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="20.0 Km" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="{x:Static t:Translation.ShipStats_RateOfFire}" Grid.Column="0"
                                   Grid.Row="1" />
                        <TextBlock Text="2.0 RPM" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="{x:Static t:Translation.ShipStats_DegreesTurn}" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="45 s" Grid.Row="2" Classes="RightSide" />
                    </Grid>
                </Expander>
                <!--AA-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="AA Defense"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <StackPanel Orientation="Vertical">
                        <DockPanel>
                            <TextBlock Text="{x:Static t:Translation.ShipStats_Range}" DockPanel.Dock="Top"></TextBlock>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                <TextBlock Text="{x:Static t:Translation.ShipStats_LongRangeAA}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_ConstantDamage}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Flak}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FlakDamage}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FlakHitchance}" Margin="20,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Right">
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                            </StackPanel>
                        </DockPanel>
                        <Separator Margin="0,5"/>
                        <DockPanel>
                            <TextBlock Text="{x:Static t:Translation.ShipStats_Range}" DockPanel.Dock="Top"></TextBlock>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                <TextBlock Text="{x:Static t:Translation.ShipStats_MediumRangeAA}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_ConstantDamage}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Flak}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FlakDamage}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FlakHitchance}" Margin="20,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Right">
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                            </StackPanel>
                        </DockPanel>
                        <Separator Margin="0,5"/>
                        <DockPanel>
                            <TextBlock Text="{x:Static t:Translation.ShipStats_Range}" DockPanel.Dock="Top"></TextBlock>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Left">
                                <TextBlock Text="{x:Static t:Translation.ShipStats_ShortRangeAA}"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_ConstantDamage}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_Flak}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FlakDamage}" Margin="20,0,0,0"/>
                                <TextBlock Text="{x:Static t:Translation.ShipStats_FlakHitchance}" Margin="20,0,0,0"/>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" DockPanel.Dock="Right">
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                                <TextBlock Text=""/>
                            </StackPanel>
                        </DockPanel>
                    </StackPanel>
