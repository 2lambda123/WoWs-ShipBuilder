                        <TextBlock Text="Detectability Range by Air" Grid.Column="0"
                                   Grid.Row="6"/>
                        <TextBlock Text="8 Km" Grid.Row="6" Classes="RightSide" />
                        <TextBlock Text="After firing main battery" Grid.Column="0"
                                   Grid.Row="7" Margin="20,0,0,0"/>
                        <TextBlock Text="20.0 Km" Grid.Row="7" Classes="RightSide" />
                        <TextBlock Text="When firing AA guns" Grid.Column="0"
                                   Grid.Row="8" Margin="20,0,0,0"/>
                        <TextBlock Text="10.0 Km" Grid.Row="8" Classes="RightSide" />
                        <TextBlock Text="When on fire" Grid.Column="0"
                                   Grid.Row="9" Margin="20,0,0,0"/>
                        <TextBlock Text="12.0 Km" Grid.Row="9" Classes="RightSide" />

                        <Separator Grid.Row="10" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Detectability from submarines" Grid.Column="0"
                                   Grid.Row="11"/>
                        <TextBlock Text="0-15 Km" Grid.Row="11" Classes="RightSide" />
                        <TextBlock Text="On the surface" Grid.Column="0"
                                   Grid.Row="12" Margin="20,0,0,0"/>
                        <TextBlock Text="15 Km" Grid.Row="12" Classes="RightSide" />
                        <TextBlock Text="At periscope depth" Grid.Column="0"
                                   Grid.Row="13" Margin="20,0,0,0"/>
                        <TextBlock Text="10 Km" Grid.Row="13" Classes="RightSide" />
                        <TextBlock Text="At operating depth" Grid.Column="0"
                                   Grid.Row="14" Margin="20,0,0,0"/>
                        <TextBlock Text="10 Km" Grid.Row="14" Classes="RightSide" />
                        <TextBlock Text="At maximum depth" Grid.Column="0"
                                   Grid.Row="15" Margin="20,0,0,0"/>
                        <TextBlock Text="0 Km" Grid.Row="15" Classes="RightSide" />
                    </Grid>
                </Expander>
                <!--Survivability-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Survivability"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="Hit Points" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="12 345" Grid.Row="0" Classes="RightSide" />

                        <Separator Grid.Row="1" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Fire Duration" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="60 s" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Max number of fires" Grid.Column="0"
                                   Grid.Row="3" />
                        <TextBlock Text="4" Grid.Row="3" Classes="RightSide" />
                        <TextBlock Text="Fire probability reduction" Grid.Column="0"
                                   Grid.Row="4" />
                        <TextBlock Text="50 %" Grid.Row="4" Classes="RightSide" />
                        <TextBlock Text="DPS per fire" Grid.Column="0"
                                   Grid.Row="5" />
                        <TextBlock Text="500" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Total damage per fire" Grid.Column="0"
                                   Grid.Row="6" />
                        <TextBlock Text="20000" Grid.Row="6" Classes="RightSide" />

                        <Separator Grid.Row="7" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Flooding duration" Grid.Column="0"
                                   Grid.Row="8" />
                        <TextBlock Text="20 s" Grid.Row="8" Classes="RightSide" />
                        <TextBlock Text="Max number of floodings" Grid.Column="0"
                                   Grid.Row="9" />
                        <TextBlock Text="2" Grid.Row="9" Classes="RightSide" />
                        <TextBlock Text="Flooding probability" Grid.Column="0"
                                   Grid.Row="10" />
                        <TextBlock Text="15 %" Grid.Row="10" Classes="RightSide" />
                        <TextBlock Text="Torpedo protection" Grid.Column="0"
                                   Grid.Row="11" />
                        <TextBlock Text="55%" Grid.Row="11" Classes="RightSide" />
                        <TextBlock Text="DPS per flooding" Grid.Column="0"
                                   Grid.Row="12" />
                        <TextBlock Text="500" Grid.Row="12" Classes="RightSide" />
                        <TextBlock Text="Total damage per flood" Grid.Column="0"
                                   Grid.Row="13" />
                        <TextBlock Text="20000" Grid.Row="13" Classes="RightSide" />
                    </Grid>
                </Expander>
            </StackPanel>
        </StackPanel>
    </ScrollViewer>
    <UserControl.Styles>
        <Style Selector="TextBlock.RightSide">
            <Setter Property="TextAlignment" Value="Right"/>
            <Setter Property="Grid.Column" Value="2"/>
        </Style>
    </UserControl.Styles>
</UserControl>
