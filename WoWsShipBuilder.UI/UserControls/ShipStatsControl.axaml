<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:t="clr-namespace:WoWsShipBuilder.UI.Translations"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WoWsShipBuilder.UI.UserControls.ShipStatsControl">
    <WrapPanel Orientation="Horizontal">
        <!--Main guns-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Main Battery"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                <TextBlock Text="Main battery setup" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="Range" Grid.Column="0"
                           Grid.Row="1" />
                <TextBlock Text="27 Km" Grid.Column="2" Grid.Row="1" TextAlignment="Right" />
                <TextBlock Text="Reload Time" Grid.Column="0"
                           Grid.Row="2" />
                <TextBlock Text="30 s" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
                <TextBlock Text="Rate of fire" Grid.Column="0"
                           Grid.Row="3" />
                <TextBlock Text="30 s" Grid.Column="2" Grid.Row="3" TextAlignment="Right" />
                <TextBlock Text="180째 turn time" Grid.Column="0"
                           Grid.Row="4" />
                <TextBlock Text="25 s" Grid.Column="2" Grid.Row="4" TextAlignment="Right" />
                <TextBlock Text="Travers speed" Grid.Column="0"
                           Grid.Row="5" />
                <TextBlock Text="10 째/s" Grid.Column="2" Grid.Row="5" TextAlignment="Right" />
                <TextBlock Text="Sigma" Grid.Column="0"
                           Grid.Row="6" />
                <TextBlock Text="2.1" Grid.Column="2" Grid.Row="6" TextAlignment="Right" />
                <TextBlock Text="Horizontal Dispersion" Grid.Column="0"
                           Grid.Row="7" />
                <TextBlock Text="200 m" Grid.Column="2" Grid.Row="7" TextAlignment="Right" />
                <TextBlock Text="Vertical Dispersion" Grid.Column="0"
                           Grid.Row="8" />
                <TextBlock Text="200 m" Grid.Column="2" Grid.Row="8" TextAlignment="Right" />
                
                <Separator Grid.Row="9" Grid.ColumnSpan="3" Margin="0,5"></Separator>

                <TextBlock Text="HE Shell name" Grid.Column="0"
                           Grid.Row="10" />
                <TextBlock Text="HE Shell" Grid.Column="2" Grid.Row="10" TextAlignment="Right" />
                <TextBlock Text="Shell Damage" Grid.Column="0"
                           Grid.Row="11" Margin="20,0,0,0"/>
                <TextBlock Text="10000" Grid.Column="2" Grid.Row="11" TextAlignment="Right" />
                <TextBlock Text="Theoretical DPM" Grid.Column="0"
                           Grid.Row="12" Margin="20,0,0,0"/>
                <TextBlock Text="1234567" Grid.Column="2" Grid.Row="12" TextAlignment="Right" />
                <TextBlock Text="Initial shell velocity" Grid.Column="0"
                           Grid.Row="13" Margin="20,0,0,0"/>
                <TextBlock Text="850 m/s" Grid.Column="2" Grid.Row="13" TextAlignment="Right" />
                <TextBlock Text="Shell weight" Grid.Column="0"
                           Grid.Row="14" Margin="20,0,0,0"/>
                <TextBlock Text="1300 Kg" Grid.Column="2" Grid.Row="14" TextAlignment="Right" />
                <TextBlock Text="HE Penetration" Grid.Column="0"
                           Grid.Row="15" Margin="20,0,0,0"/>
                <TextBlock Text="77 mm" Grid.Column="2" Grid.Row="15" TextAlignment="Right" />
                <TextBlock Text="Fire chance" Grid.Column="0"
                           Grid.Row="16" Margin="20,0,0,0"/>
                <TextBlock Text="35 %" Grid.Column="2" Grid.Row="16" TextAlignment="Right" />
                <TextBlock Text="Depth Explosion" Grid.Column="0"
                           Grid.Row="17" Margin="20,0,0,0"/>
                <TextBlock Text="4 m" Grid.Column="2" Grid.Row="17" TextAlignment="Right" />

                <Separator Grid.Row="18" Grid.ColumnSpan="3" Margin="0,5"></Separator>

                <TextBlock Text="AP Shell name" Grid.Column="0"
                           Grid.Row="19" />
                <TextBlock Text="AP Shell" Grid.Column="2" Grid.Row="19" TextAlignment="Right" />
                <TextBlock Text="Shell Damage" Grid.Column="0"
                           Grid.Row="20" Margin="20,0,0,0"/>
                <TextBlock Text="10000" Grid.Column="2" Grid.Row="20" TextAlignment="Right" />
                <TextBlock Text="Theoretical DPM" Grid.Column="0"
                           Grid.Row="21" Margin="20,0,0,0"/>
                <TextBlock Text="1234567" Grid.Column="2" Grid.Row="21" TextAlignment="Right" />
                <TextBlock Text="Initial shell velocity" Grid.Column="0"
                           Grid.Row="22" Margin="20,0,0,0"/>
                <TextBlock Text="850 m/s" Grid.Column="2" Grid.Row="22" TextAlignment="Right" />
                <TextBlock Text="Shell weight" Grid.Column="0"
                           Grid.Row="23" Margin="20,0,0,0"/>
                <TextBlock Text="1300 Kg" Grid.Column="2" Grid.Row="23" TextAlignment="Right" />
                <TextBlock Text="Richochet Angles" Grid.Column="0"
                           Grid.Row="24" Margin="20,0,0,0"/>
                <TextBlock Text="45.0 ~ 60.0" Grid.Column="2" Grid.Row="24" TextAlignment="Right" />
                <TextBlock Text="Overmatch" Grid.Column="0"
                           Grid.Row="25" Margin="20,0,0,0"/>
                <TextBlock Text="32 mm" Grid.Column="2" Grid.Row="25" TextAlignment="Right" />
                <TextBlock Text="Arming Threshold" Grid.Column="0"
                           Grid.Row="26" Margin="20,0,0,0"/>
                <TextBlock Text="77 mm" Grid.Column="2" Grid.Row="26" TextAlignment="Right" />
                <TextBlock Text="Fuse Time" Grid.Column="0"
                           Grid.Row="27" Margin="20,0,0,0"/>
                <TextBlock Text="0.033 s" Grid.Column="2" Grid.Row="27" TextAlignment="Right" />
                <TextBlock Text="Depth Explosion" Grid.Column="0"
                           Grid.Row="28" Margin="20,0,0,0"/>
                <TextBlock Text="2 m" Grid.Column="2" Grid.Row="28" TextAlignment="Right" />
            </Grid>
        </Expander>
        <!--Torpedoes-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Torpedo"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*">
                <TextBlock Text="2 X 4 533 mm Vierling" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="Range" Grid.Column="0"
                           Grid.Row="1" />
                <TextBlock Text="10 Km" Grid.Column="2" Grid.Row="1" TextAlignment="Right" />
                <TextBlock Text="Damage" Grid.Column="0"
                           Grid.Row="2" />
                <TextBlock Text="12000" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
                <TextBlock Text="Reload time" Grid.Column="0"
                           Grid.Row="3" />
                <TextBlock Text="90 s" Grid.Column="2" Grid.Row="3" TextAlignment="Right" />
                <TextBlock Text="Speed" Grid.Column="0"
                           Grid.Row="4" />
                <TextBlock Text="70 kts" Grid.Column="2" Grid.Row="4" TextAlignment="Right" />
                <TextBlock Text="Detectability range" Grid.Column="0"
                           Grid.Row="5" />
                <TextBlock Text="1.3 Km" Grid.Column="2" Grid.Row="5" TextAlignment="Right" />
                <TextBlock Text="Reaction time" Grid.Column="0"
                           Grid.Row="6" />
                <TextBlock Text="7.1 s" Grid.Column="2" Grid.Row="6" TextAlignment="Right" />
                <TextBlock Text="Flooding chance" Grid.Column="0"
                           Grid.Row="7" />
                <TextBlock Text="200 %" Grid.Column="2" Grid.Row="7" TextAlignment="Right" />
                <TextBlock Text="180째 turn time" Grid.Column="0"
                           Grid.Row="8" />
                <TextBlock Text="25 s" Grid.Column="2" Grid.Row="8" TextAlignment="Right" />
                <TextBlock Text="Travers speed" Grid.Column="0"
                           Grid.Row="9" />
                <TextBlock Text="10 째/s" Grid.Column="2" Grid.Row="9" TextAlignment="Right" />
            </Grid>
        </Expander>
        
        <!--Airstrikes-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Airstrike"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*">
                <TextBlock Text="{x:Static t:Translation.MainWindow_Range}" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="20.0 Km" Grid.Column="2" Grid.Row="0" TextAlignment="Right" />
                <TextBlock Text="{x:Static t:Translation.MainWindow_RateOfFire}" Grid.Column="0"
                           Grid.Row="1" />
                <TextBlock Text="2.0 RPM" Grid.Column="2" Grid.Row="1" TextAlignment="Right" />
                <TextBlock Text="{x:Static t:Translation.MainWindow_TurnTime}" Grid.Column="0"
                           Grid.Row="2" />
                <TextBlock Text="45 s" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
            </Grid>
        </Expander>
        <!--Secondary-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Secondary Battery"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*">
                <TextBlock Text="{x:Static t:Translation.MainWindow_Range}" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="20.0 Km" Grid.Column="2" Grid.Row="0" TextAlignment="Right" />
                <TextBlock Text="{x:Static t:Translation.MainWindow_RateOfFire}" Grid.Column="0"
                           Grid.Row="1" />
                <TextBlock Text="2.0 RPM" Grid.Column="2" Grid.Row="1" TextAlignment="Right" />
                <TextBlock Text="{x:Static t:Translation.MainWindow_TurnTime}" Grid.Column="0"
                           Grid.Row="2" />
                <TextBlock Text="45 s" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
            </Grid>
        </Expander>
        <!--AA-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="AA Defense"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                <TextBlock Text="AA Range" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="6.0 Km" Grid.Column="2" Grid.Row="0" TextAlignment="Right" />
                
                <Separator Grid.Row="1" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
                <TextBlock Text="Long Range Aura" Grid.Column="0"
                           Grid.Row="2" />
                <TextBlock Text="0.1-6.0 Km" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
                <TextBlock Text="Constant damage" Grid.Column="0"
                           Grid.Row="3" Margin="20,0,0,0"/>
                <TextBlock Text="100" Grid.Column="2" Grid.Row="3" TextAlignment="Right" />
                <TextBlock Text="Flak" Grid.Column="0"
                           Grid.Row="4" Margin="20,0,0,0"/>
                <TextBlock Text="6" Grid.Column="2" Grid.Row="4" TextAlignment="Right" />
                <TextBlock Text="Flak Damage" Grid.Column="0"
                           Grid.Row="5" Margin="20,0,0,0"/>
                <TextBlock Text="600" Grid.Column="2" Grid.Row="5" TextAlignment="Right" />
                <TextBlock Text="Flak Hitchance" Grid.Column="0"
                           Grid.Row="6" Margin="20,0,0,0"/>
                <TextBlock Text="50 %" Grid.Column="2" Grid.Row="6" TextAlignment="Right" />

                <Separator Grid.Row="7" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
                <TextBlock Text="Medium Range Aura" Grid.Column="0"
                           Grid.Row="8" />
                <TextBlock Text="0.1-3.5 Km" Grid.Column="2" Grid.Row="8" TextAlignment="Right" />
                <TextBlock Text="Constant damage" Grid.Column="0"
                           Grid.Row="9" Margin="20,0,0,0"/>
                <TextBlock Text="100" Grid.Column="2" Grid.Row="9" TextAlignment="Right" />
                <TextBlock Text="Flak" Grid.Column="0"
                           Grid.Row="10" Margin="20,0,0,0"/>
                <TextBlock Text="6" Grid.Column="2" Grid.Row="10" TextAlignment="Right" />
                <TextBlock Text="Flak Damage" Grid.Column="0"
                           Grid.Row="11" Margin="20,0,0,0"/>
                <TextBlock Text="600" Grid.Column="2" Grid.Row="11" TextAlignment="Right" />
                <TextBlock Text="Flak Hitchance" Grid.Column="0"
                           Grid.Row="12" Margin="20,0,0,0"/>
                <TextBlock Text="50 %" Grid.Column="2" Grid.Row="12" TextAlignment="Right" />

                <Separator Grid.Row="13" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
                <TextBlock Text="Short Range Aura" Grid.Column="0"
                           Grid.Row="14" />
                <TextBlock Text="0.1-2.0 Km" Grid.Column="2" Grid.Row="14" TextAlignment="Right" />
                <TextBlock Text="Constant damage" Grid.Column="0"
                           Grid.Row="15" Margin="20,0,0,0"/>
                <TextBlock Text="100" Grid.Column="2" Grid.Row="15" TextAlignment="Right" />
                <TextBlock Text="Flak" Grid.Column="0"
                           Grid.Row="16" Margin="20,0,0,0"/>
                <TextBlock Text="6" Grid.Column="2" Grid.Row="16" TextAlignment="Right" />
                <TextBlock Text="Flak Damage" Grid.Column="0"
                           Grid.Row="17" Margin="20,0,0,0"/>
                <TextBlock Text="600" Grid.Column="2" Grid.Row="17" TextAlignment="Right" />
                <TextBlock Text="Flak Hitchance" Grid.Column="0"
                           Grid.Row="18" Margin="20,0,0,0"/>
                <TextBlock Text="50 %" Grid.Column="2" Grid.Row="18" TextAlignment="Right" />
            </Grid>
        </Expander>
        <!--Maneuvrability-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Maneuvrability"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*">
                <TextBlock Text="Maximum Speed" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="27.0 knots" Grid.Column="2" Grid.Row="0" TextAlignment="Right" />
                <TextBlock Text="Full power forward" Grid.Column="0"
                           Grid.Row="1" Margin="20,0,0,0"/>
                <TextBlock Text="60.0 s" Grid.Column="2" Grid.Row="1" TextAlignment="Right" />
                <TextBlock Text="Full power backward" Grid.Column="0"
                           Grid.Row="2" Margin="20,0,0,0"/>
                <TextBlock Text="30.0 s" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
                <TextBlock Text="Power to weight ratio" Grid.Column="0"
                           Grid.Row="3" Margin="20,0,0,0"/>
                <TextBlock Text="2.06 hp/ton" Grid.Column="2" Grid.Row="3" TextAlignment="Right" />
                
                <Separator Grid.Row="4" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
                <TextBlock Text="Turning circle radius" Grid.Column="0"
                           Grid.Row="5" />
                <TextBlock Text="900m" Grid.Column="2" Grid.Row="5" TextAlignment="Right" />
                <TextBlock Text="Rudder shift time" Grid.Column="0"
                           Grid.Row="6" />
                <TextBlock Text="22.1 s" Grid.Column="2" Grid.Row="6" TextAlignment="Right" />
            </Grid>
        </Expander>
        <!--Concealment-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Concealment"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                <TextBlock Text="Detectability Range by Sea" Grid.Column="0"
                           Grid.Row="0"/>
                <TextBlock Text="20.0 Km" Grid.Column="2" Grid.Row="0" TextAlignment="Right" />
                <TextBlock Text="After firing main battery" Grid.Column="0"
                           Grid.Row="1" Margin="20,0,0,0"/>
                <TextBlock Text="20.0 Km" Grid.Column="2" Grid.Row="1" TextAlignment="Right" />
                <TextBlock Text="After firing main guns in smoke" Grid.Column="0"
                           Grid.Row="2" Margin="20,0,0,0"/>
                <TextBlock Text="10.0 Km" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
                <TextBlock Text="When firing AA guns" Grid.Column="0"
                           Grid.Row="3" Margin="20,0,0,0"/>
                <TextBlock Text="10.0 Km" Grid.Column="2" Grid.Row="3" TextAlignment="Right" />
                <TextBlock Text="When on fire" Grid.Column="0"
                           Grid.Row="4" Margin="20,0,0,0"/>
                <TextBlock Text="12.0 Km" Grid.Column="2" Grid.Row="4" TextAlignment="Right" />
                
                <Separator Grid.Row="5" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
                <TextBlock Text="Detectability Range by Air" Grid.Column="0"
                           Grid.Row="6"/>
                <TextBlock Text="8 Km" Grid.Column="2" Grid.Row="6" TextAlignment="Right" />
                <TextBlock Text="After firing main battery" Grid.Column="0"
                           Grid.Row="7" Margin="20,0,0,0"/>
                <TextBlock Text="20.0 Km" Grid.Column="2" Grid.Row="7" TextAlignment="Right" />
                <TextBlock Text="When firing AA guns" Grid.Column="0"
                           Grid.Row="8" Margin="20,0,0,0"/>
                <TextBlock Text="10.0 Km" Grid.Column="2" Grid.Row="8" TextAlignment="Right" />
                <TextBlock Text="When on fire" Grid.Column="0"
                           Grid.Row="9" Margin="20,0,0,0"/>
                <TextBlock Text="12.0 Km" Grid.Column="2" Grid.Row="9" TextAlignment="Right" />
                
                <Separator Grid.Row="10" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
