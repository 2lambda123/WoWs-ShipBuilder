                           Grid.Row="11"/>
                <TextBlock Text="0-15 Km" Grid.Column="2" Grid.Row="11" TextAlignment="Right" />
                <TextBlock Text="On the surface" Grid.Column="0"
                           Grid.Row="12" Margin="20,0,0,0"/>
                <TextBlock Text="15 Km" Grid.Column="2" Grid.Row="12" TextAlignment="Right" />
                <TextBlock Text="At periscope depth" Grid.Column="0"
                           Grid.Row="13" Margin="20,0,0,0"/>
                <TextBlock Text="10 Km" Grid.Column="2" Grid.Row="13" TextAlignment="Right" />
                <TextBlock Text="At operating depth" Grid.Column="0"
                           Grid.Row="14" Margin="20,0,0,0"/>
                <TextBlock Text="10 Km" Grid.Column="2" Grid.Row="14" TextAlignment="Right" />
                <TextBlock Text="At maximum depth" Grid.Column="0"
                           Grid.Row="15" Margin="20,0,0,0"/>
                <TextBlock Text="0 Km" Grid.Column="2" Grid.Row="15" TextAlignment="Right" />
            </Grid>
        </Expander>
        <!--Survivability-->
        <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Survivability"
                  Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
            <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                <TextBlock Text="Hit Points" Grid.Column="0"
                           Grid.Row="0" />
                <TextBlock Text="12 345" Grid.Column="2" Grid.Row="0" TextAlignment="Right" />

                <Separator Grid.Row="1" Grid.ColumnSpan="3" Margin="0,5"></Separator>

                <TextBlock Text="Fire Duration" Grid.Column="0"
                           Grid.Row="2" />
                <TextBlock Text="60 s" Grid.Column="2" Grid.Row="2" TextAlignment="Right" />
                <TextBlock Text="Max number of fires" Grid.Column="0" 
                           Grid.Row="3" />
                <TextBlock Text="4" Grid.Column="2" Grid.Row="3" TextAlignment="Right" />
                <TextBlock Text="Fire probability reduction" Grid.Column="0"
                           Grid.Row="4" />
                <TextBlock Text="50 %" Grid.Column="2" Grid.Row="4" TextAlignment="Right" />
                <TextBlock Text="DPS per fire" Grid.Column="0"
                           Grid.Row="5" />
                <TextBlock Text="500" Grid.Column="2" Grid.Row="5" TextAlignment="Right" />
                <TextBlock Text="Total damage per fire" Grid.Column="0"
                           Grid.Row="6" />
                <TextBlock Text="20000" Grid.Column="2" Grid.Row="6" TextAlignment="Right" />

                <Separator Grid.Row="7" Grid.ColumnSpan="3" Margin="0,5"></Separator>
                
                <TextBlock Text="Flooding duration" Grid.Column="0"
                           Grid.Row="8" />
                <TextBlock Text="20 s" Grid.Column="2" Grid.Row="8" TextAlignment="Right" />
                <TextBlock Text="Max number of floodings" Grid.Column="0"
                           Grid.Row="9" />
                <TextBlock Text="2" Grid.Column="2" Grid.Row="9" TextAlignment="Right" />
                <TextBlock Text="Flooding probability" Grid.Column="0"
                           Grid.Row="10" />
                <TextBlock Text="15 %" Grid.Column="2" Grid.Row="10" TextAlignment="Right" />
                <TextBlock Text="Torpedo protection" Grid.Column="0"
                           Grid.Row="11" />
                <TextBlock Text="55%" Grid.Column="2" Grid.Row="11" TextAlignment="Right" />
                <TextBlock Text="DPS per flooding" Grid.Column="0"
                           Grid.Row="12" />
                <TextBlock Text="500" Grid.Column="2" Grid.Row="12" TextAlignment="Right" />
                <TextBlock Text="Total damage per flood" Grid.Column="0"
                           Grid.Row="13" />
                <TextBlock Text="20000" Grid.Column="2" Grid.Row="13" TextAlignment="Right" />
            </Grid>
