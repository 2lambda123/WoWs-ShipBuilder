                </Expander>
            </StackPanel>
            <!--Misc-->
            <StackPanel Orientation="Vertical" Spacing="10">
                <!--Maneuvrability-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Maneuvrability"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*">
                        <TextBlock Text="Maximum Speed" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="27.0 knots" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="Full power forward" Grid.Column="0"
                                   Grid.Row="1" Margin="20,0,0,0"/>
                        <TextBlock Text="60.0 s" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="Full power backward" Grid.Column="0"
                                   Grid.Row="2" Margin="20,0,0,0"/>
                        <TextBlock Text="30.0 s" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Power to weight ratio" Grid.Column="0"
                                   Grid.Row="3" Margin="20,0,0,0"/>
                        <TextBlock Text="2.06 hp/ton" Grid.Row="3" Classes="RightSide" />

                        <Separator Grid.Row="4" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Turning circle radius" Grid.Column="0"
                                   Grid.Row="5" />
                        <TextBlock Text="900m" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Rudder shift time" Grid.Column="0"
                                   Grid.Row="6" />
                        <TextBlock Text="22.1 s" Grid.Row="6" Classes="RightSide" />
                    </Grid>
                </Expander>
                <!--Concealment-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Concealment"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="Detectability Range by Sea" Grid.Column="0"
                                   Grid.Row="0"/>
                        <TextBlock Text="20.0 Km" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="After firing main battery" Grid.Column="0"
                                   Grid.Row="1" Margin="20,0,0,0"/>
                        <TextBlock Text="20.0 Km" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="After firing main guns in smoke" Grid.Column="0"
                                   Grid.Row="2" Margin="20,0,0,0"/>
                        <TextBlock Text="10.0 Km" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="When firing AA guns" Grid.Column="0"
                                   Grid.Row="3" Margin="20,0,0,0"/>
