<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:t="clr-namespace:WoWsShipBuilder.UI.Translations"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WoWsShipBuilder.UI.UserControls.ShipStatsControl">

    <ScrollViewer>
        <StackPanel Orientation="Horizontal">
            <!-- Main Armaments -->
            <StackPanel Orientation="Vertical" Spacing="10">
                <!--Main guns-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Main Battery"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="Main battery setup" Grid.Column="0" 
                                   Grid.Row="0" />
                        <TextBlock Text="Range" Grid.Column="0"
                                   Grid.Row="1" />
                        <TextBlock Text="27 Km" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="Reload Time" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="30 s" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Rate of fire" Grid.Column="0"
                                   Grid.Row="3" />
                        <TextBlock Text="30 s" Grid.Row="3" Classes="RightSide" />
                        <TextBlock Text="180째 turn time" Grid.Column="0"
                                   Grid.Row="4" />
                        <TextBlock Text="25 s" Grid.Row="4" Classes="RightSide" />
                        <TextBlock Text="Travers speed" Grid.Column="0"
                                   Grid.Row="5" />
                        <TextBlock Text="10 째/s" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Sigma" Grid.Column="0"
                                   Grid.Row="6" />
                        <TextBlock Text="2.1" Grid.Row="6" Classes="RightSide" />
                        <TextBlock Text="Horizontal Dispersion" Grid.Column="0"
                                   Grid.Row="7" />
                        <TextBlock Text="200 m" Grid.Row="7" Classes="RightSide" />
                        <TextBlock Text="Vertical Dispersion" Grid.Column="0"
                                   Grid.Row="8" />
                        <TextBlock Text="200 m" Grid.Row="8" Classes="RightSide" />

                        <Separator Grid.Row="9" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="HE Shell name" Grid.Column="0"
                                   Grid.Row="10" />
                        <TextBlock Text="HE Shell" Grid.Row="10" Classes="RightSide" />
                        <TextBlock Text="Shell Damage" Grid.Column="0"
                                   Grid.Row="11" Margin="20,0,0,0"/>
                        <TextBlock Text="10000" Grid.Row="11" Classes="RightSide" />
                        <TextBlock Text="Theoretical DPM" Grid.Column="0"
                                   Grid.Row="12" Margin="20,0,0,0"/>
                        <TextBlock Text="1234567" Grid.Row="12" Classes="RightSide" />
                        <TextBlock Text="Initial shell velocity" Grid.Column="0"
                                   Grid.Row="13" Margin="20,0,0,0"/>
                        <TextBlock Text="850 m/s" Grid.Row="13" Classes="RightSide" />
                        <TextBlock Text="Shell weight" Grid.Column="0"
                                   Grid.Row="14" Margin="20,0,0,0"/>
                        <TextBlock Text="1300 Kg" Grid.Row="14" Classes="RightSide" />
                        <TextBlock Text="HE Penetration" Grid.Column="0"
                                   Grid.Row="15" Margin="20,0,0,0"/>
                        <TextBlock Text="77 mm" Grid.Row="15" Classes="RightSide" />
                        <TextBlock Text="Fire chance" Grid.Column="0"
                                   Grid.Row="16" Margin="20,0,0,0"/>
                        <TextBlock Text="35 %" Grid.Row="16" Classes="RightSide" />
                        <TextBlock Text="Depth Explosion" Grid.Column="0"
                                   Grid.Row="17" Margin="20,0,0,0"/>
                        <TextBlock Text="4 m" Grid.Row="17" Classes="RightSide" />

                        <Separator Grid.Row="18" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="AP Shell name" Grid.Column="0"
                                   Grid.Row="19" />
                        <TextBlock Text="AP Shell" Grid.Row="19" Classes="RightSide" />
                        <TextBlock Text="Shell Damage" Grid.Column="0"
                                   Grid.Row="20" Margin="20,0,0,0"/>
                        <TextBlock Text="10000" Grid.Row="20" Classes="RightSide" />
                        <TextBlock Text="Theoretical DPM" Grid.Column="0"
                                   Grid.Row="21" Margin="20,0,0,0"/>
                        <TextBlock Text="1234567" Grid.Row="21" Classes="RightSide" />
                        <TextBlock Text="Initial shell velocity" Grid.Column="0"
                                   Grid.Row="22" Margin="20,0,0,0"/>
                        <TextBlock Text="850 m/s" Grid.Row="22" Classes="RightSide" />
                        <TextBlock Text="Shell weight" Grid.Column="0"
                                   Grid.Row="23" Margin="20,0,0,0"/>
                        <TextBlock Text="1300 Kg" Grid.Row="23" Classes="RightSide" />
                        <TextBlock Text="Richochet Angles" Grid.Column="0"
                                   Grid.Row="24" Margin="20,0,0,0"/>
                        <TextBlock Text="45.0 ~ 60.0" Grid.Row="24" Classes="RightSide" />
                        <TextBlock Text="Overmatch" Grid.Column="0"
                                   Grid.Row="25" Margin="20,0,0,0"/>
                        <TextBlock Text="32 mm" Grid.Row="25" Classes="RightSide" />
                        <TextBlock Text="Arming Threshold" Grid.Column="0"
                                   Grid.Row="26" Margin="20,0,0,0"/>
                        <TextBlock Text="77 mm" Grid.Row="26" Classes="RightSide" />
                        <TextBlock Text="Fuse Time" Grid.Column="0"
                                   Grid.Row="27" Margin="20,0,0,0"/>
                        <TextBlock Text="0.033 s" Grid.Row="27" Classes="RightSide" />
                        <TextBlock Text="Depth Explosion" Grid.Column="0"
                                   Grid.Row="28" Margin="20,0,0,0"/>
                        <TextBlock Text="2 m" Grid.Row="28" Classes="RightSide" />
                    </Grid>
                </Expander>
                <!--Torpedoes-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Torpedo"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="2 X 4 533 mm Vierling" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="Range" Grid.Column="0"
                                   Grid.Row="1" />
                        <TextBlock Text="10 Km" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="Damage" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="12000" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Reload time" Grid.Column="0"
                                   Grid.Row="3" />
                        <TextBlock Text="90 s" Grid.Row="3" Classes="RightSide" />
                        <TextBlock Text="Speed" Grid.Column="0"
                                   Grid.Row="4" />
                        <TextBlock Text="70 kts" Grid.Row="4" Classes="RightSide" />
                        <TextBlock Text="Detectability range" Grid.Column="0"
                                   Grid.Row="5" />
                        <TextBlock Text="1.3 Km" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Reaction time" Grid.Column="0"
                                   Grid.Row="6" />
                        <TextBlock Text="7.1 s" Grid.Row="6" Classes="RightSide" />
                        <TextBlock Text="Flooding chance" Grid.Column="0"
                                   Grid.Row="7" />
                        <TextBlock Text="200 %" Grid.Row="7" Classes="RightSide" />
                        <TextBlock Text="180째 turn time" Grid.Column="0"
                                   Grid.Row="8" />
                        <TextBlock Text="25 s" Grid.Row="8" Classes="RightSide" />
                        <TextBlock Text="Travers speed" Grid.Column="0"
                                   Grid.Row="9" />
                        <TextBlock Text="10 째/s" Grid.Row="9" Classes="RightSide" />
                    </Grid>
                </Expander>

                <!--Airstrikes-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Airstrike"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="Airstrike Name" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="Type" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="Available flights" Grid.Column="0"
                                   Grid.Row="1" />
                        <TextBlock Text="2" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="Reload Time" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="45 s" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Aircraft HP" Grid.Column="0"
                                   Grid.Row="3" />
                        <TextBlock Text="2000" Grid.Row="3" Classes="RightSide" />
                        <TextBlock Text="Bombs in Payload" Grid.Column="0"
                                   Grid.Row="4" />
                        <TextBlock Text="6" Grid.Row="4" Classes="RightSide" />
                        <TextBlock Text="Bomb Type" Grid.Column="0"
                                   Grid.Row="5" />
                        <TextBlock Text="Bomb 160 Kg" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Maximum bomb damage" Grid.Column="0"
                                   Grid.Row="6" />
                        <TextBlock Text="6800" Grid.Row="6" Classes="RightSide" />
                        <TextBlock Text="Armor penetration capacity" Grid.Column="0"
                                   Grid.Row="7" />
                        <TextBlock Text="37.7 mm" Grid.Row="7" Classes="RightSide" />
                        <TextBlock Text="Depth explosion" Grid.Column="0"
                                   Grid.Row="8" />
                        <TextBlock Text="2 m" Grid.Row="8" Classes="RightSide" />
                        <TextBlock Text="Fire chance" Grid.Column="0"
                                   Grid.Row="9" />
                        <TextBlock Text="39%" Grid.Row="9" Classes="RightSide" />
                        <TextBlock Text="Range" Grid.Column="0"
                                   Grid.Row="10" />
                        <TextBlock Text="13 Km" Grid.Row="10" Classes="RightSide" />
                        <TextBlock Text="Reload Time" Grid.Column="0"
                                   Grid.Row="11" />
                        <TextBlock Text="100 s" Grid.Row="11" Classes="RightSide" />
                        <TextBlock Text="Delay" Grid.Column="0"
                                   Grid.Row="12" />
                        <TextBlock Text="15 s" Grid.Row="12" Classes="RightSide" />
                    </Grid>
                </Expander>
            </StackPanel>

            <!-- Secondary Armaments -->
            <StackPanel Orientation="Vertical" Spacing="10">
                <!--Secondary-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Secondary Battery"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*">
                        <TextBlock Text="{x:Static t:Translation.MainWindow_Range}" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="20.0 Km" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="{x:Static t:Translation.MainWindow_RateOfFire}" Grid.Column="0"
                                   Grid.Row="1" />
                        <TextBlock Text="2.0 RPM" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="{x:Static t:Translation.MainWindow_TurnTime}" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="45 s" Grid.Row="2" Classes="RightSide" />
                    </Grid>
                </Expander>
                <!--AA-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="AA Defense"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="AA Range" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="6.0 Km" Grid.Row="0" Classes="RightSide" />

                        <Separator Grid.Row="1" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Long Range Aura" Grid.Column="0"
                                   Grid.Row="2" />
                        <TextBlock Text="0.1-6.0 Km" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Constant damage" Grid.Column="0"
                                   Grid.Row="3" Margin="20,0,0,0"/>
                        <TextBlock Text="100" Grid.Row="3" Classes="RightSide" />
                        <TextBlock Text="Flak" Grid.Column="0"
                                   Grid.Row="4" Margin="20,0,0,0"/>
                        <TextBlock Text="6" Grid.Row="4" Classes="RightSide" />
                        <TextBlock Text="Flak Damage" Grid.Column="0"
                                   Grid.Row="5" Margin="20,0,0,0"/>
                        <TextBlock Text="600" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Flak Hitchance" Grid.Column="0"
                                   Grid.Row="6" Margin="20,0,0,0"/>
                        <TextBlock Text="50 %" Grid.Row="6" Classes="RightSide" />

                        <Separator Grid.Row="7" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Medium Range Aura" Grid.Column="0"
                                   Grid.Row="8" />
                        <TextBlock Text="0.1-3.5 Km" Grid.Row="8" Classes="RightSide" />
                        <TextBlock Text="Constant damage" Grid.Column="0"
                                   Grid.Row="9" Margin="20,0,0,0"/>
                        <TextBlock Text="100" Grid.Row="9" Classes="RightSide" />
                        <TextBlock Text="Flak" Grid.Column="0"
                                   Grid.Row="10" Margin="20,0,0,0"/>
                        <TextBlock Text="6" Grid.Row="10" Classes="RightSide" />
                        <TextBlock Text="Flak Damage" Grid.Column="0"
                                   Grid.Row="11" Margin="20,0,0,0"/>
                        <TextBlock Text="600" Grid.Row="11" Classes="RightSide" />
                        <TextBlock Text="Flak Hitchance" Grid.Column="0"
                                   Grid.Row="12" Margin="20,0,0,0"/>
                        <TextBlock Text="50 %" Grid.Row="12" Classes="RightSide" />

                        <Separator Grid.Row="13" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Short Range Aura" Grid.Column="0"
                                   Grid.Row="14" />
                        <TextBlock Text="0.1-2.0 Km" Grid.Row="14" Classes="RightSide" />
                        <TextBlock Text="Constant damage" Grid.Column="0"
                                   Grid.Row="15" Margin="20,0,0,0"/>
                        <TextBlock Text="100" Grid.Row="15" Classes="RightSide" />
                        <TextBlock Text="Flak" Grid.Column="0"
                                   Grid.Row="16" Margin="20,0,0,0"/>
                        <TextBlock Text="6" Grid.Row="16" Classes="RightSide" />
                        <TextBlock Text="Flak Damage" Grid.Column="0"
                                   Grid.Row="17" Margin="20,0,0,0"/>
                        <TextBlock Text="600" Grid.Row="17" Classes="RightSide" />
                        <TextBlock Text="Flak Hitchance" Grid.Column="0"
                                   Grid.Row="18" Margin="20,0,0,0"/>
                        <TextBlock Text="50 %" Grid.Row="18" Classes="RightSide" />
                    </Grid>
                </Expander>
            </StackPanel>
            <!--Misc-->
            <StackPanel Orientation="Vertical" Spacing="10">
                <!--Maneuvrability-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Maneuvrability"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*">
                        <TextBlock Text="Maximum Speed" Grid.Column="0"
                                   Grid.Row="0" />
                        <TextBlock Text="27.0 knots" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="Full power forward" Grid.Column="0"
                                   Grid.Row="1" Margin="20,0,0,0"/>
                        <TextBlock Text="60.0 s" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="Full power backward" Grid.Column="0"
                                   Grid.Row="2" Margin="20,0,0,0"/>
                        <TextBlock Text="30.0 s" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="Power to weight ratio" Grid.Column="0"
                                   Grid.Row="3" Margin="20,0,0,0"/>
                        <TextBlock Text="2.06 hp/ton" Grid.Row="3" Classes="RightSide" />

                        <Separator Grid.Row="4" Grid.ColumnSpan="3" Margin="0,5"/>

                        <TextBlock Text="Turning circle radius" Grid.Column="0"
                                   Grid.Row="5" />
                        <TextBlock Text="900m" Grid.Row="5" Classes="RightSide" />
                        <TextBlock Text="Rudder shift time" Grid.Column="0"
                                   Grid.Row="6" />
                        <TextBlock Text="22.1 s" Grid.Row="6" Classes="RightSide" />
                    </Grid>
                </Expander>
                <!--Concealment-->
                <Expander Classes="modules" ExpandDirection="Down" IsExpanded="True" Header="Concealment"
                          Padding="5" BorderBrush="Gray" BorderThickness="1" CornerRadius="5" Margin="5,0,5,0">
                    <Grid ColumnDefinitions="*,30,*" RowDefinitions="*,*,*,*,*,*,*,*,*,*,*,*,*,*,*,*">
                        <TextBlock Text="Detectability Range by Sea" Grid.Column="0"
                                   Grid.Row="0"/>
                        <TextBlock Text="20.0 Km" Grid.Row="0" Classes="RightSide" />
                        <TextBlock Text="After firing main battery" Grid.Column="0"
                                   Grid.Row="1" Margin="20,0,0,0"/>
                        <TextBlock Text="20.0 Km" Grid.Row="1" Classes="RightSide" />
                        <TextBlock Text="After firing main guns in smoke" Grid.Column="0"
                                   Grid.Row="2" Margin="20,0,0,0"/>
                        <TextBlock Text="10.0 Km" Grid.Row="2" Classes="RightSide" />
                        <TextBlock Text="When firing AA guns" Grid.Column="0"
                                   Grid.Row="3" Margin="20,0,0,0"/>
                        <TextBlock Text="10.0 Km" Grid.Row="3" Classes="RightSide" />
